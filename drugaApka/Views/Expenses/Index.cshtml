@model IEnumerable<drugaApka.Models.EXPENSES>
@{
    ViewBag.Title = "Wydatki";
}
<div class="container">
    <div class="row">
        <a class="btn btn-primary btn-lg" href="~/Expenses/SearchDetails">Szczegółowe wydatki</a><button class="btn btn-default btn-lg">Inny</button>
        <div class="col-lg-12"><font size="5"><p class="text-center"><strong>Wydatki</strong></p></font></div>
    </div>
</div>

<div class="table-responsive">
    <table class="table table-striped table-hover table-bordered">
        <thead>
            <tr>
                <th>Miesiac</th>
                <th>Wynajem</th>
                <th>Czynsz</th>
                <th>Prąd</th>
                <th>Gaz</th>
                <th>Internet</th>
                <th>Dodatkowe</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    @for (int i = 0; i < 7; i++)
                {
                    switch (i)
                    {
                        case 0:
                                <th>@item.ExpensePerMonth</th>
                                break;
                            case 1:
                                <th>@item.RentalFee</th>
                                break;
                            case 2:
                                <th>@item.Rent</th>
                                break;
                            case 3:
                                <th>@item.CurrentBill</th>
                                break;
                            case 4:
                                <th>@item.GasBill</th>
                                break;
                            case 5:
                                <th>@item.InternetBill</th>
                                break;
                            case 6:
                                <th>cos</th>
                                break;
                            default:
                                break;
                        }
                    }
                </tr>
            }
        </tbody>
    </table>
</div>

